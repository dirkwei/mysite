.sub-item-wrapper
  margin 10px 0px
  height 600px
  font-family "Microsoft YaHei UI", "Helvetica Neue", Helvetica, Arial, sans-serif
  font-size 12px
  background-color lightcyan
  .button
    width 100%
    height 30px
    margin 10px 20px
    .btn
      width 100px
      height 30px
      border none
      outline none
      text-align center
      line-height 30px
      background-color #22e9e1
      float right
      color #fff
      border-radius 5px
      margin 10px 20px
  .sub-item-wrapper
    text-align left
    ul
      list-style: none
      li
        height 30px
        line-height 30px
        span
          margin: 0px 10px
